<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://example.org/diabetes_prevention.owl"
         xmlns="http://example.org/diabetes_prevention.owl#"
         xmlns:swrl="http://www.w3.org/2003/11/swrl#">

<owl:Ontology rdf:about="http://example.org/diabetes_prevention.owl"/>

<owl:DatatypeProperty rdf:about="#hasCondition">
  <rdfs:domain rdf:resource="#Person"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasBMI">
  <rdfs:domain rdf:resource="#Person"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#recommendExercise">
  <rdfs:domain rdf:resource="#Person"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Person">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:NamedIndividual rdf:about="#John_Doe">
  <rdf:type rdf:resource="#Person"/>
  <hasCondition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">High blood sugar</hasCondition>
  <hasBMI rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">32.0</hasBMI>
  <recommendExercise rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Yes</recommendExercise>
</owl:NamedIndividual>

<swrl:Variable rdf:about="urn:swrl#p"/>

<swrl:Variable rdf:about="urn:swrl#bmi"/>

<swrl:Imp>
  <swrl:body>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#Person"/>
          <swrl:argument1 rdf:resource="urn:swrl#p"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest>
        <rdf:Description>
          <rdf:first>
            <swrl:DatavaluedPropertyAtom>
              <swrl:propertyPredicate rdf:resource="#hasCondition"/>
              <swrl:argument1 rdf:resource="urn:swrl#p"/>
              <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">High blood sugar</swrl:argument2>
            </swrl:DatavaluedPropertyAtom>
          </rdf:first>
          <rdf:rest>
            <rdf:Description>
              <rdf:first>
                <swrl:DatavaluedPropertyAtom>
                  <swrl:propertyPredicate rdf:resource="#hasBMI"/>
                  <swrl:argument1 rdf:resource="urn:swrl#p"/>
                  <swrl:argument2 rdf:resource="urn:swrl#bmi"/>
                </swrl:DatavaluedPropertyAtom>
              </rdf:first>
              <rdf:rest>
                <rdf:Description>
                  <rdf:first>
                    <swrl:BuiltinAtom>
                      <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#greaterThan"/>
                      <swrl:arguments>
                        <rdf:Description>
                          <rdf:first rdf:resource="urn:swrl#bmi"/>
                          <rdf:rest>
                            <rdf:Description>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">30</rdf:first>
                            </rdf:Description>
                          </rdf:rest>
                        </rdf:Description>
                      </swrl:arguments>
                    </swrl:BuiltinAtom>
                  </rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:Description>
              </rdf:rest>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </swrl:body>
  <swrl:head>
    <rdf:Description>
      <rdf:first>
        <swrl:DatavaluedPropertyAtom>
          <swrl:propertyPredicate rdf:resource="#recommendExercise"/>
          <swrl:argument1 rdf:resource="urn:swrl#p"/>
        </swrl:DatavaluedPropertyAtom>
      </rdf:first>
      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    </rdf:Description>
  </swrl:head>
</swrl:Imp>


</rdf:RDF>
